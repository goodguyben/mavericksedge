{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.mavericksedge.ca" }],
      "destination": "https://mavericksedge.ca/$1",
      "statusCode": 308
    },
    {
      "source": "/largest-n8n-workflow-collection/",
      "destination": "/largest-n8n-workflow-collection",
      "statusCode": 301
    },
    {
      "source": "/largest-n8n-workflow-collection/:slug/",
      "destination": "/largest-n8n-workflow-collection/:slug",
      "statusCode": 301
    },
    {
      "source": "/pricing",
      "destination": "/pricing-edmonton-web-design-marketing",
      "statusCode": 301
    },
    {
      "source": "/pricing/web",
      "destination": "/web-design-pricing-edmonton",
      "statusCode": 301
    },
    {
      "source": "/pricing-edmonton-web-design",
      "destination": "/web-design-pricing-edmonton",
      "statusCode": 301
    },
    {
      "source": "/services/digital-marketing-edmonton",
      "destination": "/digital-marketing-services-edmonton",
      "statusCode": 301
    },
    {
      "source": "/services/ai-integration-automation-edmonton",
      "destination": "/ai-automation-services-edmonton",
      "statusCode": 301
    },
    {
      "source": "/services/web-design-and-development-edmonton",
      "destination": "/web-design-services-edmonton",
      "statusCode": 301
    },
    {
      "source": "/cookie-policy-website-tracking",
      "destination": "/cookie-policy",
      "statusCode": 301
    },
    {
      "source": "/frequently-asked-questions-web-design",
      "destination": "/contact",
      "statusCode": 301
    },
    {
      "source": "/search",
      "destination": "/blog",
      "statusCode": 301
    },
    {
      "source": "/blog/web-design-automation",
      "destination": "/blog",
      "statusCode": 301
    },
    {
      "source": "/blog/chatbot-implementation-guide",
      "destination": "/blog",
      "statusCode": 301
    },
    {
      "source": "/blog/data-analytics-automation",
      "destination": "/blog",
      "statusCode": 301
    },
    {
      "source": "/blog/customer-service-automation",
      "destination": "/blog",
      "statusCode": 301
    },
    {
      "source": "/blog/ai-automation-2025-guide",
      "destination": "/blog",
      "statusCode": 301
    },
    {
      "source": "/blog/seo-automation-tools",
      "destination": "/blog",
      "statusCode": 301
    },
    {
      "source": "/blog/good-seo-is-good-ge",
      "destination": "/blog/good-seo-is-good-geo",
      "statusCode": 301
    },
    {
      "source": "/blog/marketing-automation-strategies",
      "destination": "/blog",
      "statusCode": 301
    },
    {
      "source": "/blog/good-seo-is-good",
      "destination": "/blog/good-seo-is-good-geo",
      "statusCode": 301
    },
    {
      "source": "/about-edmonton-web-design-company",
      "destination": "/about",
      "statusCode": 301
    },
    {
      "source": "/gdpr-compliance-data-protection",
      "destination": "/gdpr-compliance",
      "statusCode": 301
    },
    {
      "source": "/case-studies-edmonton-clients",
      "destination": "/work",
      "statusCode": 301
    },
    {
      "source": "/services-edmonton-alberta",
      "destination": "/services",
      "statusCode": 301
    },
    {
      "source": "/services/web",
      "destination": "/web-design-services-edmonton",
      "statusCode": 301
    },
    {
      "source": "/free-guides-digital-marketing-seo",
      "destination": "/blog",
      "statusCode": 301
    },
    {
      "source": "/blog-digital-marketing-web-development",
      "destination": "/blog",
      "statusCode": 301
    },
    {
      "source": "/services/ai",
      "destination": "/ai-automation-services-edmonton",
      "statusCode": 301
    },
    {
      "source": "/services/content-creation-strategy-edmonton",
      "destination": "/digital-marketing-services-edmonton",
      "statusCode": 301
    }
  ],
  "rewrites": [
    {
      "source": "/api/contact",
      "destination": "/api/contact.js"
    },
    {
      "source": "/api/google-reviews",
      "destination": "/api/google-reviews.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 